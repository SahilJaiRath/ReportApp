  <SQLQuery><![CDATA[SELECT CODE, HSN_NO ,PD.DESCRIPTION,PD.UNIT_OF_MEASUREMENT,PD.EXCISE_CHAPTER_NUMBER 
     , PD.UNIT_CODE,PROD_GROUP,PROD_SUB_GROUP ,pd.STATUS,PD.SPECIFICATION,PD.MARKET_TYPE,PD.MODEL_NO 
     , PD.CREATED_BY,TRUNC(PD.CREATION_DATE),PD.LAST_UPDATED_BY,TRUNC(PD.LAST_UPDATE_DATE) 
     , PD.PRODUCT_TYP, PD.REFFERAL_CODE, PD.GST,PD.PD_GSW,PD.PD_NTW,PD.BATCH_TYPE,PD.BATCH_PLANT 
     , PD.SAMP_QTY,PD.RE_TEST,PD.INSP_TYPE,PD.MRP 
     , PD.AR_REQ,PD.PD_SEC,PD.PROD_JOB_FLAG, PD.PACK_SIZE 
     , (select DISTINCT IU.UOM  from item_packing_uom IU where IU.ITEM_CD=PD.CODE AND IU.UNIT_CD=60001 AND ROWNUM=1) pack_uom 
     , (select DISTINCT  IU.CONVER_FACT from  item_packing_uom IU where IU.ITEM_CD=PD.CODE  AND IU.UNIT_CD=60001 AND ROWNUM=1) pack_conversion 
     , PD.SAP_NUMBER 
FROM   PRODUCT PD  join  PRODUCTS ON PD.CODE=PRODUCTS.PRODUCT_CODE]]></SQLQuery>
