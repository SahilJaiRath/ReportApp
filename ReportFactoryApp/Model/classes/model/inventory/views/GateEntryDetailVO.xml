  <SQLQuery><![CDATA[SELECT A.UNIT_CD,VEN_CD,PO_VEN_NAME VEN_NAME, A.GATE_ENO, A.GATE_EDT,  
DECODE(A.CHALLAN_TYPE,'MRA','MATERIAL CHALLAN','API','AGAINST PURCHASE INDENT','FOC','OTHER','REO','SALE RETURN','FGE','GENERAL NON FREE OF COST','ARG','AGAINST RETURNABLE GATE PASS','NOC','PETTY PURCHASE')CHALLAN_TYPE, 
DECODE(A.DOC_TYPE,'PUC','PURCHASE CHALLAN','API','AGAINST PURCHASE INDENT','FOC','GENERAL NON FREE COST','REO','CUSTOMER MATERIAL','FGE','GENERAL NON FREE OF COST','ARG','AGAINST RETURNABLE GATE PASS','NOC','PETTY PURCHASE')DOCUMENT_TYPE, 
DECODE(A.REQ_DOC,'P','PURCHASE','I','SALE INVOICE','J','JOB WORK','W','WITHOUT PO/INDENT','C','AGAINST CONTRACT','R','AGAINST RGP')RECEIVED_AGAINST,  
A.DOC_TYPE, DOC_NO CHALLAN_NO, DOC_DT CHALLAN_DT,B.PO_DT, 
A.BILL_NO,A.BILL_DT,B.PO_HEAD_PO_NO PO_NO, B.PO_HEAD_AMD_NO AMD_NO,B.ITEM_CD, C.ITEM_DESC,B.CHALLAN_QTY, B.CHALLAN_UOM,PROC_CD,PROC_SEQ 
,E_WB_NO EWAY_BILL_NO,E_BW_DT EWAY_BILL_DATE,E_WB_VL EWAY_BILL_VALUE,BOE_NO,BOE_DT,VEHICLE_NO,MANF_CODE 
,(SELECT NAME FROM MANUFACT_MASTER WHERE CODE=MANF_CODE) MANF_NAME 
,FRM_DESTINATION,CASH_MEMO_AMT CASH_AMOUNT, 
PO_DET_HSN,VEN_HSN_CODE,DEPT_CD,(SELECT LONG_DESC FROM LOCATION WHERE LOCAT_CODE=A.DEPT_CD) DEPT_NAME 
,EMP_CD,(SELECT ENAME FROM EMP_VIEW WHERE ENO=EMP_CD) EMP_NAME 
,PREPARED_BY,(SELECT ENAME FROM EMP_VIEW WHERE ENO=PREPARED_BY) PREPARED_BY_NAME 
,APPROVED_BY,(SELECT ENAME FROM EMP_VIEW WHERE ENO=APPROVED_BY) APPROVED_BY_NAME,DOCKET_NO GR_NO,DOCKET_DT GR_DT, 
TRNS_NAME TRANSPORTER_CODE,(SELECT NAME FROM VENDOR_MASTER WHERE VENDOR_CODE=TRNS_NAME) TRANSPORTER_NAME,CREDIT_YN CREDIT_FLAG 
FROM GT_EHEADS A,GT_EDETAILS B,ITEM_STOCK C 
WHERE A.GATE_ENO=B.GT_EHEAD_GATE_ENO 
AND B.ITEM_CD=C.ITEM_CD 
AND TRUNC(A.GATE_EDT) >= :P13_FROM_DATE 
AND TRUNC(A.GATE_EDT) <= :P13_TO_DATE 
AND A.UNIT_CD=:P13_UNIT 
ORDER BY 1,3,5,4]]></SQLQuery>
