(SELECT DESCRIPTION FROM TERMS.PART_MASTER WHERE PART_CODE=SAD.ITEM_CD)PROD_DESC,PROC_SEQ,  
PROC_CD,QTY,REMARKS,RATE,STOCK,BATCH_NO_SAD,ADJUST_TYP_SAD,  
AR_N0_SAD,BATCH_DATE_SAD,AR_DATE_SAD,MOISTURE_PER_SAD,DUST_PER_SAD,  
(select USER_NAME from user_master where EMP_CD= SAH.CREATED_BY ) Created_BY,  
  
  
(select USER_NAME from user_master where EMP_CD= SAH.APPROVED_BY)approve_by, WASTE_PER_SAD  
from terms.STOCK_ADJUST_HEAD SAH,  
terms.STOCK_ADJUST_DTL SAD where SAH.ADJUST_NO=SAD.ADJUST_NO      
AND (SAH.UNIT_CD=:P166_UNIT or '%'=:P166_UNIT) 
  
  
   AND (ITEM_CD = :P166_item OR '%' = :P166_item)  
  
ORDER BY SAD.ADJUST_NO]]></SQLQuery>
