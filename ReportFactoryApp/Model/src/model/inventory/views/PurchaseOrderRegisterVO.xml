    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <SQLQuery><![CDATA[select a.po_no,a.po_dt ,DECODE (INSTR (a.po_no, 'H'), 0, 'Works', 'Head Office')po_type,amd_no,AMD_DT,(select name from vendor_master where vendor_code=ven_cd) party_name,a.VALID_FR,a.VALID_TO,PIOS_ITEM_CD item_cd 
,PO_HSN_CODE hsn_no,item_desc,UOM,QTY,b.MATERIAL_RATE,nvl(QTY,0)*nvl(b.MATERIAL_RATE,0) Amount,PIOS_POIS_NO,SRV_HEAD_SRV_NO MRN_NO, 
(select SRV_DT from srv_heads where srv_no=d.SRV_HEAD_SRV_NO) MRN_DT,CHALLAN_QTY,RECEIPT_QTY,ACCEPT_QTY 
from po_head a,po_detail b,item_stock c, srv_details D 
where a.po_no=po_head_po_no 
and a.po_id=b.po_id 
and PIOS_ITEM_CD=c.item_cd 
and PIOS_ITEM_CD=d.item_cd(+) 
and a.po_no=d.PO_NO(+) 
and a.amd_no=d.PO_HEAD_AMD_NO(+) 
/*and amd_no=(select max(amd_no) from po_head where po_no=a.po_no)*/ 
and A.PO_DT between :P106_FROM_DATE AND :P106_TO_DATE AND A.UNIT_CD=:P106_UNIT 
order by po_dt]]></SQLQuery>
