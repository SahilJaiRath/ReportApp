VM.UNIT_CODE UNIT_CODE,VM.UNIT_CODE UNIT_NAME ,VM.REGISTERED_DEALER,VM.CREATED_BY,VM.PAY_TERM, VM.VENDOR_FLAG,VM.VENDOR_TYPE, 
VR.VENDOR_ADDRESS_TYPE,VR.CONTACT_PERSON,VR.PHONES,VR.TELEX,VR.WEEK_OFF, 
ADDRESS1 || ADDRESS2|| ADDRESS3 , VR.CITY_CODE,CITY,VR.STATE,VR.STATE_GST_CODE,COUNTRY,VR.PIN_CODE,VM.REG_FLAG,VM.INTER_UNIT,VM.PARTY_TYPE,VM.QA_STATUS, 
VM.OBJECT_VERSION_NUMBER, 
case when VM.reg_flag='R'  then 'REGISTERED' 
ELSE   'UNREGISTERED' 
END AS REGISTRATION_FLAG,VM.STAX_TYPE, 
CASE WHEN STAX_TYPE='L' THEN 'INTRASTATE' 
ELSE 'INTERSTATE' 
END AS VEN_TP  
 from vendor_master VM , 
      VENDOR_REGD_ADDRESS VR 
WHERE PARTY_TYPE = 'V'--VENDOR_TYPE='C' 
AND VM.VENDOR_CODE=VR.VENDOR_CODE]]></SQLQuery>
