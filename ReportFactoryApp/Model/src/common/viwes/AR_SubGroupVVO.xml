<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AR_SubGroupVVO"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="common.viwes.AR_SubGroupVVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P272_UNIT"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P272_LOCATION"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P272_ITEM_GROUP"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[Select SUB_GRP_CD S_GROUP, SUB_GRP_DESC S_GROUP_NAME 
  From VW_ALL_ITEM_MAS 
 Where UNIT_CD = :P272_UNIT 
   And Default_LOC = :P272_LOCATION 
   AND GROUP_CODE = DECODE(NVL(:P272_ITEM_GROUP, 'ALL'), 'ALL', GROUP_CODE, :P272_ITEM_GROUP) 
 Group By SUB_GRP_CD, SUB_GRP_DESC]]></SQLQuery>
  <ViewAttribute
    Name="SGroup"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="S_GROUP"
    Expression="S_GROUP"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="5"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="SGroupName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="80"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="S_GROUP_NAME"
    Expression="S_GROUP_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="80"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
