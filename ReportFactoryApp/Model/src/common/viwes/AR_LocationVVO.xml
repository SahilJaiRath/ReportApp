<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="AR_LocationVVO"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="common.viwes.AR_LocationVVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P272_UNIT"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_USER_NAME"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[Select LM.SHORT_DESC||'[ '||LM.LOCAT_CODE||']' LOC_NAME, LM.LOCAT_CODE 
  From Location LM 
 Where UNIT_CD Like :P272_UNIT 
   AND Exists (Select ULM.LOC_CD 
                  From LOC_MAPP_IND_ISS ULM 
                 Where UNIT_CD Like :P272_UNIT 
                   AND ULM.EMP_CD In (SELECT UM.EMP_ID 
                                        FROM USER_MASTER UM 
                                       WHERE UPPER (UM.USER_NAME) = UPPER(:P_USER_NAME)) 
                   AND LM.LOCAT_CODE = ULM.LOC_CD)]]></SQLQuery>
  <ViewAttribute
    Name="LocName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="LOC_NAME"
    Expression="LOC_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="LocatCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="6"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="LOCAT_CODE"
    Expression="LOCAT_CODE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="6"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
